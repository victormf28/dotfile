[user]
	name = usuario
	email = demo@correo.com
[credential]
	helper = cache --timeout=9800
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[apply]
	whitespace = fix
[alias]
	st = status
	pu = pull
	ck = checkout
	ckb = checkout -b
	br = branch
	brd = branch -d
	cm = commit
	ca = commit -am
	ad = add .
	me = merge
	ps = push
	or = origin
	db= branch -d
	lg = log --graph -10
	ll = log --graph -30 --pretty=format:'%Cgreen%h %Creset%ad %Cblue%an %Creset%s'
	in = fetch --dry-run
	out = push --dry-run
	last = cat-file commit HEAD
[merge]
	tool = meld
[core]
	editor = vim
	attributesfile = ~/.gitattributes
[push]
	default = simple
